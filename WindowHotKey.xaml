        <Label x:Name="label2_Copy1" Content="組合技約四分鐘施放一次" HorizontalAlignment="Left" Margin="71,267,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label x:Name="label2_Copy2" Content="主教建議配置" HorizontalAlignment="Left" Margin="11,293,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
