        <Label x:Name="label_Copy3" Content="攻擊參數*" HorizontalAlignment="Left" Margin="146,7,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_AttackParam" HorizontalAlignment="Left" Height="23" Margin="204,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" TextChanged="textBox_AttackParam_TextChanged" Text="50"/>

